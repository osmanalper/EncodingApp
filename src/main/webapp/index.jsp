<html>
<head>
    <title>Base29 Encoding - Decoding</title>
    <link rel="stylesheet" type="text/css" href="imports/css/screen.css">
    <script type="text/javascript" src="imports/js/jquery-1.12.0.js"></script>
    <script type="text/javascript" src="imports/js/index.js"></script>
</head>
<body>
    <div class="center-div">
        <h2>Base29 Encoding - Decoding</h2>
        <div>
            <table>
                <tr>
                    <td>
                        Encode Value:
                    </td>
                    <td>
                        <textarea id="encodeValue"></textarea>
                        <input type="submit" value="Encode" id="btnEncode">
                    </td>
                </tr>
                <tr>
                    <td>
                        Encoding Result:
                    </td>
                    <td>
                        <textarea id="encodingRes" disabled="disabled"></textarea>
                    </td>
                </tr>
            </table>
        </div>
        <hr>
        <div>
            <table>
                <tr>
                    <td>
                        Decode Value:
                    </td>
                    <td>
                        <textarea id="decodeValue"></textarea>
                        <input type="submit" value="Decode" id="btnDecode">
                    </td>
                </tr>
                <tr>
                    <td>
                        Decoding Result:
                    </td>
                    <td>
                        <textarea id="decodingRes" disabled="disabled"></textarea>
                    </td>
                </tr>
            </table>
        </div>
    </div>
</body>
</html>
